<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/bottom_sheet_detail_prodi"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="16dp"
    android:orientation="vertical">

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      android:layout_marginBottom="16dp"
      android:weightSum="2">

    <TextView
        android:layout_width="0dp"
        android:layout_weight="0.7"
        android:layout_height="match_parent"
        android:text="@string/nama_prodi_text"
        android:fontFamily="@font/robotoregular"
        android:textSize="14sp"/>
    <TextView
        android:id="@+id/tv_nama_prodi_detail_prodi"
        android:layout_width="0dp"
        android:layout_weight="1.3"
        android:layout_height="match_parent"
        android:fontFamily="@font/robotoblack"
        android:textSize="14sp"/>
  </LinearLayout>
  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      android:layout_marginBottom="16dp"
      android:weightSum="2">

    <TextView
        android:layout_width="0dp"
        android:layout_weight="0.7"
        android:layout_height="match_parent"
        android:text="@string/ptn_text"
        android:fontFamily="@font/robotoregular"
        android:textSize="14sp"/>
    <TextView
        android:id="@+id/tv_nama_ptn_detail_prodi"
        android:layout_width="0dp"
        android:layout_weight="1.3"
        android:layout_height="match_parent"
        android:fontFamily="@font/robotoblack"
        android:textSize="14sp"/>
  </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      android:layout_marginBottom="16dp"
      android:weightSum="2">

    <TextView
        android:layout_width="0dp"
        android:layout_weight="0.7"
        android:layout_height="match_parent"
        android:text="@string/tipe_text"
        android:fontFamily="@font/robotoregular"
        android:textSize="14sp"/>
    <TextView
        android:id="@+id/tv_tipe_prodi_detail_prodi"
        android:layout_width="0dp"
        android:layout_weight="1.3"
        android:layout_height="match_parent"
        android:fontFamily="@font/robotoblack"
        android:textSize="14sp"/>
  </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      android:layout_marginBottom="16dp"
      android:weightSum="2">

    <TextView
        android:layout_width="0dp"
        android:layout_weight="0.7"
        android:layout_height="match_parent"
        android:text="@string/akreditasi_ptn_text"
        android:fontFamily="@font/robotoregular"
        android:textSize="14sp"/>
    <TextView
        android:id="@+id/tv_akreditasi_prodi_detail_prodi"
        android:layout_width="0dp"
        android:layout_weight="1.3"
        android:layout_height="match_parent"
        android:fontFamily="@font/robotoblack"
        android:textSize="14sp"/>
  </LinearLayout>
  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      android:layout_marginBottom="16dp"
      android:weightSum="2">

    <TextView
        android:layout_width="0dp"
        android:layout_weight="0.7"
        android:layout_height="match_parent"
        android:text="@string/daya_tampung_text"
        android:fontFamily="@font/robotoregular"
        android:textSize="14sp"/>
    <TextView
        android:id="@+id/tv_daya_tampung_detail_prodi"
        android:layout_width="0dp"
        android:layout_weight="1.3"
        android:layout_height="match_parent"
        android:fontFamily="@font/robotoblack"
        android:textSize="14sp"/>
  </LinearLayout>
  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      android:layout_marginBottom="16dp"
      android:weightSum="2">

    <TextView
        android:layout_width="0dp"
        android:layout_weight="0.7"
        android:layout_height="match_parent"
        android:text="@string/daya_tampung_2018_text"
        android:fontFamily="@font/robotoregular"
        android:textSize="14sp"/>
    <TextView
        android:id="@+id/tv_daya_tampung_2018_detail_prodi"
        android:layout_width="0dp"
        android:layout_weight="1.3"
        android:layout_height="match_parent"
        android:fontFamily="@font/robotoblack"
        android:textSize="14sp"/>
  </LinearLayout>
  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      android:layout_marginBottom="16dp"
      android:weightSum="2">

    <TextView
        android:layout_width="0dp"
        android:layout_weight="0.7"
        android:layout_height="match_parent"
        android:text="@string/persentase_kursi_text"
        android:fontFamily="@font/robotoregular"
        android:textSize="14sp"/>
    <TextView
        android:id="@+id/tv_persentase_kursi_detail_prodi"
        android:layout_width="0dp"
        android:layout_weight="1.3"
        android:layout_height="match_parent"
        android:fontFamily="@font/robotoblack"
        android:textSize="14sp"/>
  </LinearLayout>
  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:orientation="horizontal"
      android:layout_marginBottom="16dp"
      android:weightSum="2">

    <TextView
        android:layout_width="0dp"
        android:layout_weight="0.7"
        android:layout_height="match_parent"
        android:text="@string/jenis_portofolio_text"
        android:fontFamily="@font/robotoregular"
        android:textSize="14sp"/>
    <TextView
        android:id="@+id/tv_jenis_portofolio_detail_prodi"
        android:layout_width="0dp"
        android:layout_weight="1.3"
        android:layout_height="match_parent"
        android:fontFamily="@font/robotoblack"
        android:textSize="14sp"/>
  </LinearLayout>
</LinearLayout>